    "abandoned": {
        "kduma/cron": true,
        "kduma/dotenv-filler": true,
        "kduma/permissions": true,
        "kduma/coinhive-api": true,
        "kduma/eloquent-guidable": "kduma/eloquent-uuidable"
    },

    "require-all": true,

    "archive": {
        "directory": "dist",
        "skip-dev": true
    },

    "s3-satis": {
        "plugins": {
            "skip-remote-versions": true,
            
            "file-restrictions-map-generator": {
                "enabled": true,
                "extra-json": true
            },
            
            "remove-fields-from-json": {
                "enabled": true,
                "remove": [
                    "source"
                ]
            }
        }
    }
